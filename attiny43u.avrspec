OPTION	REFS		0		# REFS[0]
OPTION	MUX		2		# MUX[2:0]
OPTION	ADCSRB_ADLAR			# ADCSRB.ADLAR
OPTION	TCCR0B_CS0X			# TCCR0B.CS0x
OPTION	PROGMEM_SIZE	4096
OPTION	RAM_SIZE	256
OPTION	EEPROM_SIZE	64

#	Func		Channel
ADC_MUX	ADC0		0		# 0b000
ADC_MUX	ADC1		1		# 0b001
