ADC_MUX	ADC3		3		# 0b011
ADC_MUX	GND		4		# 0b100 (0v GND)
ADC_MUX	REF_1_1		5		# 0b101 (1.1v Internal Reference)
ADC_MUX	VBAT		6		# 0b110
ADC_MUX	ADC4		7		# 0b111 (Temperature)

#	Type	Name			Value
MAP	REFS	VCC			0	# 0b0
MAP	REFS	REF_1_1			1	# 0b1 (1.1v Internal Ref)

MAP	WGM0	NORMAL:0xFF		0
